<p id="notice"><%= notice %></p>

<h1>Automatic Registration Form</h1>

<table>
  <thead>
    <tr>
      <th>first_name</th>
      <th>last_name</th>
      <th>mobile_number</th>
      <th>course_period</th>
      <th>start_date</th>
      <th>payment_type</th>


      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @automatics.each do |automatic| %>
      <tr>
        <td><%= automatic.first_name %></td>
        <td><%= automatic.last_name %></td>
        <td><%= automatic.mobile_number %></td>
        <td><%= automatic.course_period %></td>
        <td><%= automatic.start_date %></td>
        <td><%= automatic.payment_type %></td>
        <td><%= link_to 'Show', automatic %></td>
        <td><%= link_to 'Edit', edit_automatic_path(automatic) %></td>
        <td><%= link_to 'Destroy', automatic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration form', new_automatic_path %>
