<p id="notice"><%= notice %></p>

<h1>Automatic Registration Form</h1>

<table>
  <thead>
    <tr>
      <th>first_name</th>
      <th>last_name</th>
      <th>mobile_number</th>
      <th>course_period</th>
      <th>start_date</th>
      <th>payment_type</th>


      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manuals.each do |manual| %>
      <tr>
        <td><%= manual.first_name %></td>
        <td><%= manual.last_name %></td>
        <td><%= manual.mobile_number %></td>
        <td><%= manual.course_period %></td>
        <td><%= manual.start_date %></td>
        <td><%= manual.payment_type %></td>
        <td><%= link_to 'Show', manual %></td>
        <td><%= link_to 'Edit', edit_manual_path(manual) %></td>
        <td><%= link_to 'Destroy', manual, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registration form', new_manual_path %>
